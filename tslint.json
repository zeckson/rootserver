{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "no-console": false,
    "object-literal-sort-keys": [
      false,
      "match-declaration-order",
      "shorthand-first"
    ],
    "quotemark": [
      true,
      "single"
    ]
  },
  "rulesDirectory": []
}
